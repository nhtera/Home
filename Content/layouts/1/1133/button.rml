<rml:array id="size">
<rml:item name="Small" id="small" png="small.png" round="5" spng="small.png"/>
<rml:item name="Medium" id="medium" png="medium.png" round="8" spng="med.png"/>
<rml:item name="Large" id="large" png="large.png" round="12" spng="large.png"/>
</rml:array>

<rml:array id="shape">
<rml:item name="Square" id="square" icon="-light.png" class="no" inner="no" bevel="button-inner-" color="bg" colover="bg" border="no"/>
<rml:item name="Square Outline" id="square" icon="-light-outline.png" class="no" inner="no" bevel="button-inner-" color="border" colover="bg" border="outline"/>
<rml:item name="Rounded" id="rounded" icon="-light-round.png" class="round" inner="round" bevel="button-inner-" color="bg" colover="bg" border="no"/>
<rml:item name="Rounded Outline" id="rounded" icon="-light-outline-round.png" class="round" inner="round" bevel="button-inner-" color="border" colover="bg" border="outline"/>
<rml:item name="3D" id="3d" icon="-light.png" class="button-shadow no" inner="no" bevel="button-bevel-" color="bg" colover="bg" border="no"/>
<rml:item name="Rounded 3D" id="rounded3d" icon="-light-round.png" class="button-shadow round" inner="round" bevel="button-bevel-" color="bg" colover="bg" border="no"/>
</rml:array>

<rml:array id="delete">
<rml:item name="Delete" icon="delete.png" class=""/>
<rml:item name="Delete Circle" icon="deletecircle.png" class="-circle"/>
</rml:array>

<rml:button name="$rml.colors: Button: $rml.size: Regular: $rml.shape" icon="/content/layouts/1/1133/files/icons/$rml.colors.id$rml.shape.icon">
	<rml:default>
		<div class="btn-default button-default-$rml.size.id $rml.shape.class$rml.size.round $rml.shape.color-$rml.colors.id-light $rml.shape.border-$rml.size.id">
			<div class="$rml.shape.bevel$rml.size.id $rml.shape.inner$rml.size.round">
				<div class="button-text-$rml.size.id"><rml:text/></div>
				<div class="clear"></div>
			</div>
		</div>
	</rml:default>
	<rml:mouseover>
		<div class="btn-mouseover button-mouseover-$rml.size.id $rml.shape.class$rml.size.round $rml.shape.colover-$rml.colors.id-2-light $rml.shape.border-$rml.size.id">
			<div class="$rml.shape.bevel$rml.size.id $rml.shape.inner$rml.size.round">
				<div class="button-text-$rml.size.id"><rml:text/></div>
				<div class="clear"></div>
			</div>
		</div>
	</rml:mouseover>
	<rml:disabled>
		<div class="btn-disabled button-disabled-$rml.size.id $rml.shape.class$rml.size.round $rml.shape.color-$rml.colors.id-4-dark $rml.shape.border-$rml.size.id">
			<div class="$rml.shape.bevel$rml.size.id $rml.shape.inner$rml.size.round">
				<div class="button-text-$rml.size.id"><rml:text/></div>
				<div class="clear"></div>
			</div>
		</div>
	</rml:disabled>
</rml:button>

