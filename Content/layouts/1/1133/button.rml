<rml:item name="Darker Grey" id="co6" color="#FFFFFF" link="#4AE3FF" active="#00BEE1"/>
<rml:item name="Deep Grey" id="co7" color="#FFFFFF" link="#06DAFF" active="#01F5DD"/>
